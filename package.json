{
  "name": "@vlah.io/ngx-success-box",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 5180",
    "build": "ng build ngx-success-box --prod",
    "test": "ng test",
    "test-lib": "ng test ngx-success-box",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "fix-public-api": "node ./scan.js && git add ./projects/ngx-success-box/src/public-api.ts",
    "fix-readme": "cp ./README.md ./projects/ngx-success-box/README.md && git add ./projects/ngx-success-box/README.md",
    "fix-version-patch": "cd projects/ngx-success-box && npm version patch && cd ../../ && npm run git-add-version",
    "fix-version-minor": "cd projects/ngx-success-box && npm version minor && cd ../../ && npm run git-add-version",
    "fix-version-major": "cd projects/ngx-success-box && npm version major && cd ../../ && npm run git-add-version",
    "fix-assets": "cp -r ./projects/ngx-success-box/src/assets/ ./dist/ngx-success-box/assets/",
    "git-add-version": "git add ./projects/ngx-success-box/package.json",
    "git-commit": "git commit -m \"...\"",
    "git-push": "git push && git push --tags",
    "prep-release": "npm run fix-public-api && npm run fix-readme",
    "prep-package": "npm run lint && npm run prep-release && npm run build",
    "preversion": "npm run lint && npm run prep-release",
    "version": "",
    "postversion": "npm run build && npm run fix-assets && npm run publish-release && npm run git-push",
    "make-patch-release": "npm run fix-version-patch && npm run git-commit && npm version patch",
    "make-minor-release": "npm run fix-version-minor && npm run git-commit && npm version minor",
    "make-major-release": "npm run fix-version-major && npm run git-commit && npm version major",
    "publish-release": "cd dist/ngx-success-box && npm publish --access=public && cd ../../"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "@vlah.io/ngx-worker": "^5.0.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-eslint/builder": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.3",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.3",
    "@angular-eslint/schematics": "^0.8.0-beta.3",
    "@angular-eslint/template-parser": "0.8.0-beta.3",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@types/jasmine": "~3.6.2",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "typescript": "~4.0.5"
  },
  "description": "Set of reusable Angular components (factory workers) to help display success messages.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vlah-io/ngx-success-box.git"
  },
  "author": "@vlah.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vlah-io/ngx-success-box/issues"
  },
  "homepage": "https://github.com/vlah-io/ngx-success-box#readme"
}
